025E:  CLRF   x7F
....................    unsigned int8 n = 0x00; 
....................  
....................    /* Read the data port */ 
....................    n |= input(LCD_DATA4); 
0260:  BSF    F94.4
0262:  MOVLW  00
0264:  BTFSC  F82.4
0266:  MOVLW  01
0268:  IORWF  x7F,F
....................    n |= input(LCD_DATA5) << 1; 
026A:  BSF    F94.5
026C:  MOVLW  00
026E:  BTFSC  F82.5
0270:  MOVLW  01
0272:  MOVWF  00
0274:  BCF    FD8.0
0276:  RLCF   00,F
0278:  MOVF   00,W
027A:  IORWF  x7F,F
....................    n |= input(LCD_DATA6) << 2; 
027C:  BSF    F94.6
027E:  MOVLW  00
0280:  BTFSC  F82.6
0282:  MOVLW  01
0284:  MOVWF  00
0286:  RLCF   00,F
0288:  RLCF   00,F
028A:  MOVLW  FC
028C:  ANDWF  00,F
028E:  MOVF   00,W
0290:  IORWF  x7F,F
....................    n |= input(LCD_DATA7) << 3; 
0292:  BSF    F94.7
0294:  MOVLW  00
0296:  BTFSC  F82.7
0298:  MOVLW  01
029A:  MOVWF  00
029C:  RLCF   00,F
029E:  RLCF   00,F
02A0:  RLCF   00,F
02A2:  MOVLW  F8
02A4:  ANDWF  00,F
02A6:  MOVF   00,W
02A8:  IORWF  x7F,F
....................     
....................    return(n); 
02AA:  MOVFF  7F,01
....................   #else 
02AE:  RETURN 0
....................    return(lcd.data); 
....................   #endif 
.................... } 
